{
  "name": "material-ui-dropzone",
  "version": "3.6.0",
  "description": "A Material-UI file-upload dropzone",
  "license": "MIT",
  "author": "Yuval",
  "repository": {
    "type": "git",
    "url": "https://github.com/Yuvaleros/material-ui-dropzone.git"
  },
  "keywords": [
    "react",
    "react-component",
    "material design",
    "material-ui",
    "dropzone",
    "upload",
    "react upload"
  ],
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/material-ui-dropzone.umd.cjs",
  "module": "./dist/material-ui-dropzone.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/material-ui-dropzone.js",
      "require": "./dist/material-ui-dropzone.umd.cjs"
    }
  },
  "scripts": {
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "build": "vite build",
    "start": "vite",
    "prepare": "yarn build",
    "docs:dev": "styleguidist server",
    "docs:build": "styleguidist build",
    "predocs:deploy": "yarn docs:build",
    "docs:deploy": "gh-pages -d styleguide"
  },
  "dependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "clsx": "^1.0.2",
    "react-dropzone": "^10.2.1",
    "rollup-plugin-size-snapshot": "^0.12.0"
  },
  "peerDependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.19",
    "@mui/material": "^5.14.20"
  },
  "devDependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.19",
    "@mui/material": "^5.14.20",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@types/react-dom": "^18.2.22",
    "@types/rollup-plugin-size-snapshot": "^0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "gh-pages": "^1.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-styleguidist": "^13.1.1",
    "rollup-plugin-cpy": "^2.0.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "vite": "^5.1.6"
  },
  "packageManager": "yarn@4.0.2"
}
